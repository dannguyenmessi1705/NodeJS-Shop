<section class="pagination-dan">
    <nav aria-label="Page navigation">
        <ul class="pagination justify-content-center">
            <% if (curPage !== 1 && prevPage !== 1) { %>
                <li class="page-item">
                    <a class="page-link" href="?page=1">1</a>
                </li>
            <% } %>
  
            <% if (hasPrevPage) { %>
                <li class="page-item">
                    <a class="page-link" href="?page=<%=prevPage%>"><%=prevPage%></a>
                </li>
            <% } %>
  
            <li class="page-item active">
                <a class="page-link" href="?page=<%=curPage%>"><%=curPage%></a>
            </li>
  
            <% if (hasNextPage) { %>
                <li class="page-item">
                    <a class="page-link" href="?page=<%=nextPage%>"><%=nextPage%></a>
                </li>
            <% } %>
  
            <% if (curPage !== lastPage && nextPage !== lastPage) { %>
                <li class="page-item">
                    <a class="page-link" href="?page=<%=lastPage%>"><%=lastPage%></a>
                </li>
            <% } %>
        </ul>
    </nav>
</section>